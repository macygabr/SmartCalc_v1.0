#test arifmetic1
  char str[255] = "1+1";
  double res = calc(str);
  double res_orig = 2;
  ck_assert_double_eq_tol(res_orig, res, 1e-07);


#test arifmetic2
  char str[255] = "(asin(1)-(sin(-1)))";
  float res = calc(str);
  float res_orig = 2.41226731;
  ck_assert_double_eq_tol(res_orig, res, 1e-07);

#test arifmetic3

  char str[255] = "(acos(0)-(sin(1)))";
  float res = calc(str);
  float res_orig = 0.72932534;
  ck_assert_double_eq_tol(res_orig, res, 1e-07);

#test arifmetic4
  char str[255] = "asin(1)-atan(1)+acos(1)";
  float res = calc(str);
  float res_orig = 0.78539816;
  ck_assert_double_eq_tol(res_orig, res, 1e-07);

#test arifmetic5
  char str[255] = "sin(0.5)+16-3^8";
  float res = calc(str);
  float res_orig = -6544.52057446;
  ck_assert_double_eq_tol(res_orig, res, 1e-07);

#test arifmetic6
  char str[255] = "-(-8*9.0)";
  float res = calc(str);
  float res_orig = 72.00000000;
  ck_assert_double_eq_tol(res_orig, res, 1e-07);

#test arifmetic7
  char str[255] = "(-2+0.5)*(-2)";
  float res = calc(str);
  float res_orig = 3;
  ck_assert_double_eq_tol(res_orig, res, 1e-07);

#test arifmetic8
  char str[255] = "(2*1)-(sin(1))";
  float res = calc(str);
  float res_orig = 1.15852902;
  ck_assert_double_eq_tol(res_orig, res, 1e-07);

